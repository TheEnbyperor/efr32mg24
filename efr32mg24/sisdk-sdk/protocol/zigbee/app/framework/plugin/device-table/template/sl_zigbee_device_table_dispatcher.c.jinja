#include "stack/include/sl_zigbee.h"

{# DEVICE_LEFT -#}
{%- for cb in zigbee_af_device_table_callback %}
{%- if cb['callback_type'] == "device_left" %}
  void {{ cb['function_name'] }}(sl_802154_long_addr_t newNodeEui64);
{%- endif %}
{%- endfor %}

void sli_zigbee_af_device_table_device_left_callback(sl_802154_long_addr_t newNodeEui64)
{
{%- for cb in zigbee_af_device_table_callback %}
{%- if cb['callback_type'] == "device_left" %}
  {{ cb['function_name'] }}(newNodeEui64);
{%- endif %}
{%- endfor %}
}